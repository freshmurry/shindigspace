<section class="">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="panel panel-default">
          <div class="panel-heading">This Week's Revenue</div>
          <div class="panel-body">
            <div class="container">
              <div class="container text-center">
                <% if @reservations.blank? %>
                  <h3>You don't have any bookings</h3><br/>
                  <%= link_to "List an Event Space to Earn", new_venue_path, class: "btn btn-normal" %>
                <% else %>
                  <%= column_chart @this_week_revenue, colors: ["#16A085"] %>
                <% end %>
              </div>
            </div>  
          </div>
        </div>
      </div>
    </div>
  </div>
</section>  